{
    "configurations": [
        {
            "name": "Linux",
            "compilerPath": "/usr/bin/arm-none-eabi-gcc",
            "includePath": [
                "/usr/include/newlib",
                "${workspaceFolder}/firmware/kernel",
                "${workspaceFolder}/firmware/mcu/stm32f407vg/vendor/CMSIS"
            ],
            "defines": [
                "STM32F407xx",
                "BAREMETAL"
            ],
            "cStandard": "c99",
            "cppStandard": "c++11",
            "intelliSenseMode": "linux-gcc-arm"
        },
        {
            "name": "Win",
            "includePath": [
                "C:/arm_tool/gcc/arm-none-eabi/include",
                "${workspaceFolder}/**",
                "${workspaceFolder}/firmware/kernel",
                "${workspaceFolder}/firmware/mcu/stm32f407vg/vendor/CMSIS"
            ],
            "defines": [
                "BAREMETAL",
                "STM32F407xx"
            ],
            "compilerPath": "C:/arm_tool/gcc/bin/arm-none-eabi-gcc.exe",
            "cStandard": "c99",
            "cppStandard": "gnu++11",
            "intelliSenseMode": "${default}"
        }
    ],
    "version": 4
}